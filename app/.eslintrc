{
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2020
  },
  "plugins": [
    "react-hooks"
  ],
  "extends": [
    "@atlassian/eslint-config-xen"
  ],
  "rules": {
    "no-console": "warn",
    "react-hooks/rules-of-hooks": "error"
  }
}
